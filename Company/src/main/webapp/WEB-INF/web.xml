<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>addController</servlet-name>
  	<display-name>addController</display-name>
  	<description></description>
  	<servlet-class>Controller.addController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>loginServlet</servlet-name>
  	<display-name>loginServlet</display-name>
  	<description></description>
  	<servlet-class>Controller.loginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>addOrderController</servlet-name>
  	<display-name>addOrderController</display-name>
  	<description></description>
  	<servlet-class>Controller.addOrderController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>addController</servlet-name>
  	<url-pattern>/addController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>loginServlet</servlet-name>
  	<url-pattern>/loginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>addOrderController</servlet-name>
  	<url-pattern>/addOrderController</url-pattern>
  </servlet-mapping>
</web-app>
